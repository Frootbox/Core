
<div class="form-group">
    <label for="field-{{ field.getId() }}">{{ field.getTitle() }}{% if field.getMandatory() %}<span class="mandatory">*</span>{% endif %}</label>
    <input {% if field.isRequired() %}required{% endif %} type="date" class="form-control" id="field-{{ field.getId() }}" placeholder="{% if renderer.options.placeholderLabel %}{{ field.getTitle() }}{% if field.isRequired() %}*{% endif %}{% endif %}" name="fields[{{ field.getId() }}]" {# aria-describedby="emailHelp" #}>

    {#
    <!--<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>-->
    #}
</div>

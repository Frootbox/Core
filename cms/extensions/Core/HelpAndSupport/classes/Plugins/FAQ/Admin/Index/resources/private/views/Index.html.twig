{% set v = view.getViewHelper('\\Frootbox\\Admin\\Viewhelper\\GeneralPurpose') %}

<h2>
    F.A.Q.
    <span class="headoptions">
		<a data-toggle="tooltip" data-modal data-title="neue Frage hinzufügen" href="{{ plugin.getAdminUri('Question', 'ajaxModalCompose') }}"><i class="fa fa-plus-circle"></i></a>
	</span>
</h2>

<p><a class="ajax" href="{{ v.getUri('Page', 'ajaxPanelContent', { 'pageId' : plugin.getPageId() }) }}"><i class="fa fa-chevron-circle-left"></i> zurück</a></p>

{{ v.injectPartial('\\Frootbox\\Admin\\View\\Partials\\Navigation\\Buttons', { links: [
    {
        url: plugin.getAdminUri('Import', 'index'),
        icon: 'far fa-file-import',
        title: 'Import',
        disabled: true
    },
    {
        url: plugin.getAdminUri('Configuration', 'index'),
        icon: 'far fa-cog',
        title: 'Konfiguration'
    }
]}) | raw }}


<div id="questionsReceiver">
    {{ v.injectPartial('/Frootbox/Ext/Core/HelpAndSupport/Plugins/FAQ/Admin/Question/Partials/ListQuestions', {
        plugin : plugin
    }) | raw }}
</div>




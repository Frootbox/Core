{% set v = view.getViewHelper('\\Frootbox\\Admin\\Viewhelper\\GeneralPurpose') %}

<h2>
    Newsverwaltung
    <span class="headoptions">
		<a data-toggle="tooltip" data-modal data-title="neuen Newsartikel hinzufügen" href="{{ plugin.getAdminUri('Article', 'ajaxModalCompose') }}"><i class="fa fa-plus-circle"></i></a>

        <span class="dropdown">
            <i class="far fa-bars dropdown-toggle" data-toggle="dropdown"></i>

            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <a class="ajax dropdown-item" href="{{ plugin.getAdminUri('Import', 'index') }}">Importieren</a>
            </div>
        </span>
	</span>
</h2>

<p><a class="ajax" href="{{ v.getUri('Page', 'ajaxPanelContent', { 'pageId' : plugin.getPageId() }) }}"><i class="fa fa-chevron-circle-left"></i> zurück</a></p>

{{ v.injectPartial('\\Frootbox\\Admin\\View\\Partials\\Navigation\\Buttons', { links: [
    {
        url: plugin.getAdminUri('Configuration', 'index'),
        icon: 'fa-newspaper',
        title: 'Artikel'
    },
    {
        url: plugin.getAdminUri('Categories', 'index'),
        icon: 'fa-sitemap',
        title: 'Kategorien'
    },
    {
        url: plugin.getAdminUri('Configuration', 'index'),
        icon: 'fa-cog',
        title: 'Konfiguration'
    }
]}) | raw }}

<div id="articlesNewReceiver">
    {{ v.injectPartial('/Frootbox/Ext/Core/News/Plugins/News/Admin/Article/Partials/ListArticles', {
        headline: "Artikel",
        plugin : plugin
    }) | raw }}
</div>
{% set i = view.getViewhelper('Delegator', { object: plugin }) %}

<h4 data-editable data-uid="{{ article.getUid('title') }}">{{ article.getTitle }}</h4>

<p class="text-center">{{ article.getDateStart | date('d.m.Y') }}</p>

<div data-editable data-uid="{{ article.getUid('teaser') }}"></div>

<br />

<figure data-type="Images/PictureFull" data-uid="{{ article.getUid('image')}}">
    <picture class="fluid">
        <img width="600" src="{{ i.getDummyImage({ width: 600, height: null }) }}" alt="" />
    </picture>
    <figcaption></figcaption>
</figure>
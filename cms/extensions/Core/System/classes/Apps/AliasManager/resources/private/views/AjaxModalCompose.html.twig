{% set v = view.getViewHelper('\\Frootbox\\Admin\\Viewhelper\\GeneralPurpose') %}

{{ view.set({
    'frame' : false,
}) }}

<form method="post" class="ajax" action="{{ app.getUri('ajaxCreate') }}">

    <div class="modal-body">

        <div class="form-group">
            <label for="alias">Alias</label>
            <input type="text" class="form-control" name="alias" required id="alias" placeholder="Alias">

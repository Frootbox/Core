<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%  for alias in aliases %}
        <url>
            <loc>{{  alias.getUri() }}</loc>
            <lastmod>{{  alias.getUpdated() | date('Y-m-d') }}</lastmod>
        </url>
    {%  endfor %}
</urlset>
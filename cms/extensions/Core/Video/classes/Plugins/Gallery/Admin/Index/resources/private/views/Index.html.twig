{% set v = view.getViewHelper('\\Frootbox\\Admin\\Viewhelper\\GeneralPurpose') %}

<h2>
    Video-Galerie
    <span class="headoptions">
		<a data-toggle="tooltip" data-modal data-title="neues Video hinzufügen" href="{{ plugin.getAdminUri('Video', 'ajaxModalCompose') }}"><i class="fa fa-plus-circle"></i></a>
	</span>
</h2>

<p><a class="ajax" href="{{ v.getUri('Page', 'ajaxPanelContent', { pageId: plugin.getPageId() }) }}"><i class="fa fa-chevron-circle-left"></i> zurück</a></p>

<div id="videosReceiver">
    {{ v.injectPartial('/Frootbox/Ext/Core/Video/Plugins/Gallery/Admin/Video/Partials/ListVideos', {
        plugin: plugin,
        highlight: get.get('highlight')
    }) | raw }}
</div>